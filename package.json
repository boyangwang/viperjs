{
  "name": "viperjs",
  "version": "0.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "full-build": "npm install && node ./node_modules/webpack/bin/webpack.js && node ./node_modules/mocha/bin/mocha --recursive && node ./node_modules/eslint/bin/eslint.js --fix .",
    "test": "node ./node_modules/mocha/bin/mocha --recursive",
    "webpack": "node ./node_modules/webpack/bin/webpack.js --watch",
    "eslint": "node ./node_modules/eslint/bin/eslint.js --fix",
    "eslint-all": "node ./node_modules/eslint/bin/eslint.js --fix .",
    "start": "forever start -c 'node --use-strict' -a -l ./viperjs.log -o ./viperjs.log -e ./viperjs.log --uid viperjs app.js",
    "stop": "forever stop viperjs"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyangwang/viperjs.git"
  },
  "author": "boyang",
  "contributors": [
    "boyang",
    "yingchi"
  ],
  "license": "CC-BY-NC-4.0",
  "bugs": {
    "url": "https://github.com/boyangwang/viperjs/issues"
  },
  "homepage": "https://github.com/boyangwang/viperjs#readme",
  "dependencies": {
    "esprima": "^3.0.0",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "casperjs": "^1.1.3",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "forever": "^0.15.2",
    "mocha": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "redux-devtools": "^3.3.1",
    "style-loader": "^0.13.1"
  }
